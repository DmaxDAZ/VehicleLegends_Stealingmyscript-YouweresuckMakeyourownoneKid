car:SetPrimaryPartCFrame(TweenValue.Value)
end)
local OnTween = TweenService:Create(TweenValue, TweenInfoToUse, {Value=car.PrimaryPart.CFrame+car.PrimaryPart.CFrame.LookVector*1500})
OnTween:Play()
OnTween.Completed:Wait()
end
    end)

local Queque = Tabs.Races:AddParagraph({
    Title = "Racing Queue Status",
    Content = table.concat(RacesQueueData, "\n")
})

local textLabel
for _, v in pairs(Queque) do
    if typeof(v) == "Instance" and v:IsA("TextLabel") then
        textLabel = v
        break
    end
end

local function getParagraphLabel(paragraph)
    if typeof(paragraph) == "table" then
        for _, v in pairs(paragraph) do
            if typeof(v) == "Instance" and v:IsA("TextLabel") then
                return v
            end
        end
    elseif typeof(paragraph) == "Instance" and paragraph:IsA("TextLabel") then
        return paragraph
    end
    return nil
end

spawn(function()
    RunService.Heartbeat:Connect(function()
        local RaceFolder = workspace:FindFirstChild("Races")
        if RaceFolder then
        RacesQueueData[2] = RaceFolder:FindFirstChild("Air Race"):GetAttribute("RaceActive") and "Air Race  游릭" or "Air Race  游댮"
        RacesQueueData[3] = RaceFolder:FindFirstChild("Around The Map"):GetAttribute("RaceActive") and "Around The Map  游릭" or "Around The Map  游댮"
        RacesQueueData[4] = RaceFolder:FindFirstChild("Beach Dash"):GetAttribute("RaceActive") and "Beach Dash  游릭" or "Beach Dash  游댮"
        RacesQueueData[5] = RaceFolder:FindFirstChild("Boat Race"):GetAttribute("RaceActive") and "Boat Race  游릭" or "Boat Race  游댮"
        RacesQueueData[6] = RaceFolder:FindFirstChild("Circuit Moto Race"):GetAttribute("RaceActive") and "Circuit Moto Race  游릭" or "Circuit Moto Race  游댮"
        RacesQueueData[7] = RaceFolder:FindFirstChild("Circuit Race"):GetAttribute("RaceActive") and "Circuit Race  游릭" or "Circuit Race  游댮"
        RacesQueueData[8] = RaceFolder:FindFirstChild("Desert Race"):GetAttribute("RaceActive") and "Desert Race  游릭" or "Desert Race  游댮"
        RacesQueueData[9] = RaceFolder:FindFirstChild("Drag Race"):GetAttribute("RaceActive") and "Drag Race  游릭" or "Drag Race  游댮"
        RacesQueueData[10] = RaceFolder:FindFirstChild("Highway Race"):GetAttribute("RaceActive") and "Highway Race  游릭" or "Highway Race  游댮"
        RacesQueueData[11] = RaceFolder:FindFirstChild("Miami City Circuit"):GetAttribute("RaceActive") and "Miami City Circuit 游릭" or "Miami City Circuit  游댮"
        RacesQueueData[12] = RaceFolder:FindFirstChild("Mountain Dash"):GetAttribute("RaceActive") and "Mountain Dash  游릭" or "Mountain Dash  游댮"
        RacesQueueData[13] = RaceFolder:FindFirstChild("Offroad Race"):GetAttribute("RaceActive") and "Offroad Race  游릭" or "Offroad Race  游댮"
        RacesQueueData[14] = RaceFolder:FindFirstChild("Tropical Dash"):GetAttribute("RaceActive") and "Tropical Dash  游릭" or "Tropical Dash  游댮"
        textLabel = getParagraphLabel(Queque)
        if textLabel then
            textLabel.Text = table.concat(RacesQueueData, "\n")
        end
        end
    end)
end)






user.CharacterAdded:Connect(function(newChar)
	char = newChar
	WaitUntilAlive()
	root = char:FindFirstChild("HumanoidRootPart")
	task.wait(1)
end)

Window:SelectTab(1)

SaveManager:SetLibrary(Fluent)
InterfaceManager:SetLibrary(Fluent)

SaveManager:IgnoreThemeSettings()

InterfaceManager:BuildInterfaceSection(Tabs.Ui)
SaveManager:BuildConfigSection(Tabs.Ui)

Fluent:Notify({
    Title = "Info",
    Content = "The script has been loaded.",
    Duration = 4
})
