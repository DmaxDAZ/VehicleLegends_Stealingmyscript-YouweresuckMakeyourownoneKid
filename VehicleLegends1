local Rss = game:GetService("ReplicatedStorage")
local Li = game:GetService("Lighting")
local plr = game:GetService("Players")
local user, player = plr.LocalPlayer, plr.LocalPlayer
local char = user.Character or user.CharacterAdded:Wait()
local root = char:FindFirstChild("HumanoidRootPart")
local TweenService = game:GetService("TweenService")
local Workspace = game:GetService("Workspace")
local Vehicles = Workspace:WaitForChild("Vehicles")
local RunService = game:GetService("RunService")

local checkpointsFolder = game:GetService("Workspace").RaceProps:FindFirstChild("Checkpoints")

local RacesQueueData = {
    " ",
    "Air Race N/A",
    "Around The Map N/A",
    "Beach Dash N/A",
    "Boat Race N/A",
    "Circuit Moto Race N/A",
    "Circuit Race N/A",
    "Desert Race N/A",
    "Drag Race N/A",
    "Highway Race N/A",
    "Miami City Circuit N/A",
    "Mountain Dash N/A",
    "Offroad Race N/A",
    "Tropical Dash N/A"
}


_G.FarmSped = 250

local function WaitUntilAlive()
	repeat
		task.wait()
	until user.Character and user.Character:FindFirstChild("HumanoidRootPart") and user.Character:FindFirstChildOfClass("Humanoid")
	char = user.Character
	root = char:FindFirstChild("HumanoidRootPart")
end

local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()
 
local Window = Fluent:CreateWindow({
    Title = "Anchor Hub",
    SubTitle = "EXAMPLE SCRIPT NOT RELEASE YET YOU FOOL.",
    TabWidth = 160,
    Size = UDim2.fromOffset(500, 320),
    Acrylic = false,
    Theme = "Aqua",
    MinimizeKey = Enum.KeyCode.K
})
 
local Options = Fluent.Options
 
local Tabs = {
    Home = Window:AddTab({ Title = "Home", Icon = "user" }),
    Setting = Window:AddTab({ Title = "Setting", Icon = "settings" }),
    Race = Window:AddTab({ Title = "Vehicle racing", Icon = "flag" }),
    BoatRace = Window:AddTab({ Title = "Boat racing", Icon = "sailboat" }),
    PlaneRace = Window:AddTab({ Title = "Plane racing", Icon = "plane" }),
    Vehi = Window:AddTab({ Title = "Vehicle", Icon = "car" }),
    Player = Window:AddTab({ Title = "Player", Icon = "users" }),
    Races = Window:AddTab({ Title = "Races Status", Icon = "file" }),
    Ui = Window:AddTab({ Title = "Ui Setting", Icon = "paintbrush" })
}


        Tabs.Home:AddButton({
        Title = "Copy CFrame",
        Description = "lol.",
        Callback = function()
        setclipboard(root.CFrame)

        Fluent:Notify({
         Title = "Info",
         Content = "Your humanoidrootpart cframe has been copied.",
         Duration = 5
         })
        end
    })


        Tabs.Home:AddButton({
        Title = "Copy Position",
        Description = "lol.",
        Callback = function()
        setclipboard(root.Position)

        Fluent:Notify({
         Title = "Info",
         Content = "Your humanoidrootpart position has been copied.",
         Duration = 5
         })
        end
    })

    local speed = Tabs.Setting:AddDropdown("FarmSped", {
        Title = "Auto Race Speed",
        Values = {"250", "300", "325", "350"},
        Multi = false,
        Default = 1,
    })

    speed:OnChanged(function(Value)
        if Value == "250" then
            _G.FarmSped = 250
        elseif Value == "300" then
            _G.FarmSped = 300
        elseif Value == "325" then
            _G.FarmSped = 325
        elseif Value == "350" then
            _G.FarmSped = 350
        end
    end)

    local highlight1

    local circuit = Tabs.Race:AddToggle("CircuitRace", {Title = "Auto Circuit Race [Best for money]", Default = false })

    circuit:OnChanged(function(bool)

    local car = game:GetService("Workspace").Vehicles:FindFirstChild(game.Players.LocalPlayer.Name.."'s Car")
if bool then
    if not highlight1 then
        highlight1 = Instance.new("Highlight")
        highlight1.Parent = car
        highlight1.FillColor = Color3.fromRGB(0, 0, 255)
        highlight1.OutlineColor = Color3.fromRGB(255, 255, 255)
        highlight1.FillTransparency = 0.5
        highlight1.OutlineTransparency = 0
        highlight1.DepthMode = Enum.HighlightDepthMode.Occluded
    end
else
    if highlight1 then
        highlight1:Destroy()
        highlight1 = nil
    end
end

    getfenv().Circuit = (bool and true or false)
while getfenv().Circuit do
    if getfenv().Circuit == false and not game:GetService("Workspace").Races:WaitForChild("Circuit Race"):GetAttribute("RaceActive") then return end
task.wait()
if game:GetService("Players").LocalPlayer.PlayerGui.Races.Timer.Visible == true and game:GetService("Players").LocalPlayer.PlayerGui.Races.Timer.Text == "1" and game:GetService("Workspace").Races:WaitForChild("Circuit Race"):GetAttribute("RaceActive") then
task.wait(1)
local checks = #game:GetService("Workspace").RaceProps:FindFirstChild("Checkpoints"):GetChildren()
for a=1,2 do
for i=1,checks do
local checkpointsFolder = game:GetService("Workspace").RaceProps:FindFirstChild("Checkpoints")
if not checkpointsFolder then break end

local checkpoint = checkpointsFolder:FindFirstChild(tostring(i))
if not checkpoint then break end

local dist = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - checkpoint.Display:FindFirstChildOfClass("Model").Border.Position).magnitude

local car = game:GetService("Workspace").Vehicles:FindFirstChild(game.Players.LocalPlayer.Name.."'s Car")
 local TweenService = game:GetService("TweenService")
local TweenInfoToUse = TweenInfo.new(dist/_G.FarmSped, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut, 0, false, 0)
local TweenValue = Instance.new("CFrameValue")
TweenValue.Value = car:GetPrimaryPartCFrame()        
TweenValue.Changed:Connect(function()
    if not game:GetService("Workspace").Races:WaitForChild("Circuit Race"):GetAttribute("RaceActive") then
    return
    end
    car:WaitForChild("VehicleSeat").AssemblyLinearVelocity = Vector3.new(0,1,0)
    car:PivotTo(TweenValue.Value)
end)    
if not game:GetService("Workspace").Races:WaitForChild("Circuit Race"):GetAttribute("RaceActive") then
break
end            
local OnTween = TweenService:Create(TweenValue, TweenInfoToUse, {Value=checkpointsFolder[tostring(i)].Display:FindFirstChildOfClass("Model").Border.CFrame * CFrame.new(0,3.5,0)})
OnTween:Play()
 OnTween.Completed:Wait()
end
end
elseif game:GetService("Players").LocalPlayer.PlayerGui.Races.Timer.Visible == false and not checkpointsFolder and user:DistanceFromCharacter(Vector3.new(535, 6, -1592)) > 15 then
    local car = game:GetService("Workspace").Vehicles:FindFirstChild(game.Players.LocalPlayer.Name.."'s Car")
car:WaitForChild("VehicleSeat").AssemblyLinearVelocity = Vector3.zero
car:PivotTo(CFrame.new(535, 6, -1592))
wait(1)
end
end
    end)

