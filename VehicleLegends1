for i=1,checks do
local checkpointsFolder = game:GetService("Workspace").RaceProps:FindFirstChild("Checkpoints")
if not checkpointsFolder then break end

local checkpoint = checkpointsFolder:FindFirstChild(tostring(i))
if not checkpoint then break end

local dist = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - checkpoint.Display:FindFirstChildOfClass("Model").Border.Position).magnitude

local car = game:GetService("Workspace").Vehicles:FindFirstChild(game.Players.LocalPlayer.Name.."'s Car")
 local TweenService = game:GetService("TweenService")
local TweenInfoToUse = TweenInfo.new(dist/300, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut, 0, false, 0)
local TweenValue = Instance.new("CFrameValue")
TweenValue.Value = car:GetPrimaryPartCFrame()        
TweenValue.Changed:Connect(function()
    if not game:GetService("Workspace").Races:WaitForChild("Drag Race"):GetAttribute("RaceActive") then
    return
    end
    car:WaitForChild("VehicleSeat").AssemblyLinearVelocity = Vector3.new(0,1,0)
    car:PivotTo(TweenValue.Value)
end)            
if not game:GetService("Workspace").Races:WaitForChild("Drag Race"):GetAttribute("RaceActive") then
break
end
local OnTween = TweenService:Create(TweenValue, TweenInfoToUse, {Value=checkpointsFolder[tostring(i)].Display:FindFirstChildOfClass("Model").Border.CFrame * CFrame.new(0,3.5,0)})
OnTween:Play()
 OnTween.Completed:Wait()
end
end
